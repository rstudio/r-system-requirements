{
  "patterns": ["\\bproj\\b", "\\bproj\\.4\\b"],
  "dependencies": [
    {
      "packages": ["libproj-dev"],
      "constraints": [
        {
          "os": "linux",
          "distribution": "ubuntu"
        },
        {
          "os": "linux",
          "distribution": "debian"
        }
      ]
    },
    {
      "packages": ["proj-devel"],
      "pre_install": [
        {
          "command": "yum install -y epel-release"
        }
      ],
      "constraints": [
        {
          "os": "linux",
          "distribution": "centos"
        }
      ]
    },
    {
      "packages": ["proj-devel"],
      "pre_install": [
        {
          "command": "rpm -q epel-release || yum install -y https://dl.fedoraproject.org/pub/epel/epel-release-latest-6.noarch.rpm"
        }
      ],
      "constraints": [
        {
          "os": "linux",
          "distribution": "redhat",
          "versions": ["6"]
        }
      ]
    },
    {
      "packages": ["proj-devel"],
      "pre_install": [
        {
          "command": "rpm -q epel-release || yum install -y https://dl.fedoraproject.org/pub/epel/epel-release-latest-7.noarch.rpm"
        }
      ],
      "constraints": [
        {
          "os": "linux",
          "distribution": "redhat",
          "versions": ["7"]
        }
      ]
    },
    {
      "packages": ["libproj-devel", "proj"],
      "constraints": [
        {
          "os": "linux",
          "distribution": "opensuse"
        }
      ]
    },
    {
      "packages": ["libproj-devel", "proj"],
      "pre_install": [
        {
          "command": "zypper addrepo https://download.opensuse.org/repositories/openSUSE:/Backports:/SLE-12/standard/openSUSE:Backports:SLE-12.repo"
        },
        {
          "command": "zypper --gpg-auto-import-keys refresh"
        }
      ],
      "constraints": [
        {
          "os": "linux",
          "distribution": "sle",
          "versions": ["12.3"]
        }
      ]
    }
  ]
}